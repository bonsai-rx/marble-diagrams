  [2016/01/10 v0.1 TikZ style package for drawing reactive extension marble diagrams]

\RequirePackage{tikz}
\tikzset{every picture/.style={/utils/exec={\sffamily}}}
\usetikzlibrary[arrows,shapes,decorations.text]
\definecolor{lightgray}{RGB}{174,174,174}

\tikzset{
 box/.style={draw,rectangle,right,thick,minimum height=0.719cm},
 rounded/.style={draw,rounded rectangle,right,thick},
 time/.style={thick,-angle 45},
 fdash/.style={dashed,-triangle 45},
 rdash/.style={dashed,triangle 45-},
 subscribe/.style={dashed,angle 60-,thick,color=blue},
 red/.style={fill=red},
 yellow/.style={fill=yellow},
 green/.style={fill=green},
 cyan/.style={fill=cyan},
 blue/.style={fill=blue},
 magenta/.style={fill=magenta},
 orange/.style={fill=orange},
 marble/.style={draw=black,fill=lightgray,thick,minimum size=4.5mm},
 triangle/.style={regular polygon,regular polygon sides=3,rotate=180,scale=0.8,yshift=-0.7mm},
 pentagon/.style={regular polygon,regular polygon sides=5},
 hexagon/.style={regular polygon,regular polygon sides=6},
 heptagon/.style={regular polygon,regular polygon sides=7},
 fivestar/.style={star,star point ratio=2.5,inner sep=0pt,rounded corners=0.2mm},
 buffer/.style={draw,rounded rectangle,fill=white,inner sep=3pt},
